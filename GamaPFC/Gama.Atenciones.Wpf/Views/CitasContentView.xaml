<UserControl x:Class="Gama.Atenciones.Wpf.Views.CitasContentView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True"
             xmlns:local="clr-namespace:Gama.Atenciones.Wpf.Views"
             mc:Ignorable="d" 
             xmlns:CustomControls="clr-namespace:Gama.Atenciones.Wpf.Controls"
             d:DesignHeight="325.685" d:DesignWidth="572.26">
  <Grid Margin="0,39,0,0">
    <CustomControls:CustomCalendar
      Style="{StaticResource CustomCalendarGlobalStyle}"
      Grid.Row="1" Appointments="{Binding Citas}"/>
  </Grid>
</UserControl>
